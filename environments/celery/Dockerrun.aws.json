{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "celery_beat",
      "image": "306439459454.dkr.ecr.us-west-2.amazonaws.com/backend_base",
      "command": [
        "./celery_beat.sh"
      ],
      "essential": true,
      "memory": 300
    },
    {
      "name": "celery_flower",
      "image": "306439459454.dkr.ecr.us-west-2.amazonaws.com/backend_base",
      "command": [
        "./celery_flower.sh"
      ],
      "essential": true,
      "memory": 300,
      "portMappings": [
        {
          "hostPort": 5555,
          "containerPort": 5555
        }
      ]
    }
  ]
}
